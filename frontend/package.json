  "scripts": {
    "start": "yarn run local:start",
    "build": "yarn run local:build",
    "relay": "relay-compiler --src ./ --schema ../api/schema.gql",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "standard": "standard --fix --env jest ./**/*.js",
    "local:start": "yarn run local:client && yarn run local:server && nodemon server/dist/server.js --watch server/dist/server.js",
    "local:build": "yarn run local:build:server && yarn run local:build:client",
    "local:build:client": "APP_CONFIG='local' NODE_ENV='development' webpack --bail --config config/webpack.config.client.js",
    "local:build:server": "APP_CONFIG='local' NODE_ENV='development' webpack --bail --config config/webpack.config.server.js",
    "local:client": "APP_CONFIG='local' NODE_ENV='development' webpack --display-error-details --config config/webpack.config.client.js --watch &",
    "local:server": "APP_CONFIG='local' NODE_ENV='development' webpack --display-error-details --config config/webpack.config.server.js --watch &",
    "dev:start": "yarn run dev:client && yarn run dev:server && nodemon server/dist/server.js --watch server/dist/server.js",
    "dev:build": "yarn run dev:build:server && yarn run dev:build:client",
    "dev:build:client": "APP_CONFIG='development' NODE_ENV='production' webpack --bail -p --config config/webpack.config.client.js",
    "dev:build:server": "APP_CONFIG='development' NODE_ENV='production' webpack --bail -p --config config/webpack.config.server.js",
    "dev:client": "APP_CONFIG='development' NODE_ENV='production' webpack -p --display-error-details --config config/webpack.config.client.js --watch &",
    "dev:server": "APP_CONFIG='development' NODE_ENV='production' webpack -p --display-error-details --config config/webpack.config.server.js --watch &",
    "staging:start": "yarn run staging:client && yarn run staging:server && nodemon server/dist/server.js --watch server/dist/server.js",
    "staging:build": "yarn run staging:build:server && yarn run staging:build:client",
    "staging:build:client": "APP_CONFIG='staging' NODE_ENV='production' webpack --bail -p --config config/webpack.config.client.js",
    "staging:build:server": "APP_CONFIG='staging' NODE_ENV='production' webpack --bail -p --config config/webpack.config.server.js",
    "staging:client": "APP_CONFIG='staging' NODE_ENV='production' webpack -p --display-error-details --config config/webpack.config.client.js --watch &",
    "staging:server": "APP_CONFIG='staging' NODE_ENV='production' webpack -p --display-error-details --config config/webpack.config.server.js --watch &",
    "prod:start": "yarn run prod:client && yarn run prod:server && nodemon server/dist/server.js --watch server/dist/server.js",
    "prod:build": "yarn run prod:build:server && yarn run prod:build:client",
    "prod:build:client": "APP_CONFIG='production' NODE_ENV='production' webpack --bail -p --config config/webpack.config.client.js",
    "prod:build:server": "APP_CONFIG='production' NODE_ENV='production' webpack --bail -p --config config/webpack.config.server.js",
    "prod:client": "APP_CONFIG='production' NODE_ENV='production' webpack -p --display-error-details --config config/webpack.config.client.js --watch &",
    "prod:server": "APP_CONFIG='production' NODE_ENV='production' webpack -p --display-error-details --config config/webpack.config.server.js --watch &"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.js",
      "!**/dist/**",
      "!**/coverage/**"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
